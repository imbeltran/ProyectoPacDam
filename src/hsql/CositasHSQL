//////////////////////////
//CREACION BASE DE DATOS//
//////////////////////////

DROP DATABASE IF EXISTS PacDamBD;

CREATE DATABASE PacDamBD;

USE PacDamBD;

DROP TABLE IF EXISTS Mapas;
DROP TABLE IF EXISTS Puntuaciones;
DROP TABLE IF EXISTS Partidas;
DROP TABLE IF EXISTS Configuracion;
DROP TABLE IF EXISTS Calculos;

CREATE TABLE Mapas (
    MapaID INT PRIMARY KEY,
    Nombre VARCHAR(255),
    DatosMapa VARCHAR(500),
    Estado BOOLEAN,
    NivelInfinito INT
);

CREATE TABLE Puntuaciones (
    Nombre VARCHAR(255),
    Puntuacion INT
);

CREATE TABLE Partidas (
    PartidaID INT PRIMARY KEY,
    MapaID INT NOT NULL,
    SkinID INT NOT NULL
);

CREATE TABLE Configuracion (
    Sonido BOOLEAN
);

CREATE TABLE Calculos (
    Sumas INT
);

